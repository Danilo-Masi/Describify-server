{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/waitlist",
      "dest": "/routes/waitlistRoutes.js"
    },
    {
      "src": "/api/resend",
      "dest": "/routes/remailSendRoutes.js"
    },
    {
      "src": "/health",
      "dest": "/index.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}